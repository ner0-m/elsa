@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

set(_dependend_components @ELSA_COMPONENT_DEPENDENCIES@)

foreach(_comp ${_dependend_components}) 
	find_dependency(${_comp} REQUIRED)
    if (${_comp}_FOUND)
	    include(${CMAKE_CURRENT_LIST_DIR}/${_comp}Targets.cmake)
    endif(${_comp}_FOUND) 
endforeach()
